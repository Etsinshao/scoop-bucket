    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/92.0/win64/en-US/Firefox%20Setup%2092.0.exe#/dl.7z",
            "hash": "sha512:2b686cd841e0b7cb5cd6664ed3034f39f662eb0a60be3f86511defc9e77051a96e20c8404a8be0f7efe985151de8b10c60edaca6b3c2dee5048cfa0bab33d3ce"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/92.0/win32/en-US/Firefox%20Setup%2092.0.exe#/dl.7z",
            "hash": "sha512:2b686cd841e0b7cb5cd6664ed3034f39f662eb0a60be3f86511defc9e77051a96e20c8404a8be0f7efe985151de8b10c60edaca6b3c2dee5048cfa0bab33d3ce"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/en-US/firefox/notes/",
        "regex": "<div class=\"c-release-version\">([\\d.]+)</div>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512'\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512'\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
