{
    "version": "86.0.1",
    "description": "Popular open source web browser",
    "homepage": "https://www.mozilla.org/firefox/",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/86.0.1/win64/en-US/Firefox%20Setup%2086.0.1.exe#/dl.7z",
            "hash": "sha512:b2b04101d08bec958102bfe32096c3d055b47e7598dc117a4dcbd294a214946fa10ea66516a28fb7ac54743950ce0ed1eb9d432dfac605562f82440ee2824218"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/86.0.1/win32/en-US/Firefox%20Setup%2086.0.1.exe#/dl.7z",
            "hash": "sha512:b2b04101d08bec958102bfe32096c3d055b47e7598dc117a4dcbd294a214946fa10ea66516a28fb7ac54743950ce0ed1eb9d432dfac605562f82440ee2824218"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/en-US/firefox/notes/",
        "regex": "<div class=\"c-release-version\">([\\d.]+)</div>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512'\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512'\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
