    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/98.0.1/win64/en-US/Firefox%20Setup%2098.0.1.exe#/dl.7z",
            "hash": "sha512:0b4ea18d97a270580395068d53df15bdd5f51bb1f9b56a1ea1819df4e508b44b957f997864ab901bab0f9cfbdb35505866bd9394614448ab7f3178124eebb334"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/98.0.1/win32/en-US/Firefox%20Setup%2098.0.1.exe#/dl.7z",
            "hash": "sha512:0b4ea18d97a270580395068d53df15bdd5f51bb1f9b56a1ea1819df4e508b44b957f997864ab901bab0f9cfbdb35505866bd9394614448ab7f3178124eebb334"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/en-US/firefox/notes/",
        "regex": "<div class=\"c-release-version\">([\\d.]+)</div>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512'\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512'\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
