        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/92.0.1/win32/en-US/Firefox%20Setup%2092.0.1.exe#/dl.7z",
            "hash": "sha512:46fc128cf49550fcda536399effaa709f46dbb6b338c8134eaf2bac06fe9e50e91fd6f38d84663aab17c5baa04e6d06cc460391829324fef1a5c61d13bb0c433"
