{
    "version": "89.0",
    "description": "Popular open source web browser",
    "homepage": "https://www.mozilla.org/firefox/",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/89.0/win64/en-US/Firefox%20Setup%2089.0.exe#/dl.7z",
            "hash": "sha512:2aacd910e242f4d3a149d6b0fb773d1f1b64510ad33860b39a50e77f2f362d71fed8a4aa369bab3d56388b9f787bb2563e292ef14ab7438fa40410acdea261f8"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/89.0/win32/en-US/Firefox%20Setup%2089.0.exe#/dl.7z",
            "hash": "sha512:2aacd910e242f4d3a149d6b0fb773d1f1b64510ad33860b39a50e77f2f362d71fed8a4aa369bab3d56388b9f787bb2563e292ef14ab7438fa40410acdea261f8"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/en-US/firefox/notes/",
        "regex": "<div class=\"c-release-version\">([\\d.]+)</div>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512'\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512'\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
